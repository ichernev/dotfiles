! Common terminal stuff
*scrollBar: 0
*foreground: gray
*color4: #5C5CFF
*color12: #5C5CFF
*saveLines: 1000000
*scrollTtyOutput: false
*scrollWithBuffer: true
*scrollTtyKeypress: true

! normal terminal
urxvt*font: xft:DejaVu Sans Mono-12
urxvt*background: #000000

! scratchpad
scratchpad*font: xft:Monospace-10
scratchpad*background: #111111

! C-= and C--
URxvt.keysym.C-0x3d: command:\033]50;xft:Droid Sans Mono-13\007
! URxvt.keysym.C-0x3d: command:\033]50;xft:Oxygen Mono-12:hintstyle=hintnone:antialias=1\007
URxvt.keysym.C-0:    command:\033]50;xft:Droid Sans Mono-12\007
URxvt.keysym.C-0x2d: command:\033]50;xft:Droid Sans Mono-8\007
! URxvt.perl-ext-common:  default,matcher
! URxvt.urlLauncher:      /usr/bin/chromium
! URxvt.matcher.button:   1

XTerm*renderFont: true
XTerm*faceName: xft:Droid Sans Mono-10
XTerm*background: #000000

